<section class="home">
    {{> header}}
    <section class="home__main">
        <form class="home__form" action="/photo" method="GET">
            <input id='Search' class="home__main__input" type="text" value="" aria-label="" name="gif"
                placeholder="Type Gif Name" required>
            <input id='submit' class="home__main__search" type="submit" value="Search" aria-label="">
        </form>
        <h1 class="status">{{status}}</h1>
        <div class="gifs">
            {{#each images}}
            <div class="gifs__gif">
                <img src="{{this.imagesUrl}}" alt="image" class="gifs__image">
                <h4 class="gifs__title">{{this.imagesTitle}}</h4>
            </div>
            {{/each}}
        </div>
    </section>
    {{> footer}}
</section>